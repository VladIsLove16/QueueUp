<Window x:Class="QueueUp.Window1"
               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QueueUp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Время" HorizontalAlignment="Left" Margin="106,41,0,0" TextWrapping="Wrap" Text="Время" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="Группа" HorizontalAlignment="Left" Margin="107,73,0,0" TextWrapping="Wrap" Text="Группа" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="Предмет" HorizontalAlignment="Left" Margin="106,104,0,0" TextWrapping="Wrap" Text="Предмет" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="Преподаватель" HorizontalAlignment="Left" Margin="105,133,0,0" TextWrapping="Wrap" Text="Преподаватель" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged"/>
    </Grid>
</Window>
